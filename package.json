  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "concurrently": "^9.1.2",
    "electron": "^35.0.2",
    "electron-builder": "^25.1.8",
    "eslint": "^9",
    "eslint-config-next": "15.2.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
