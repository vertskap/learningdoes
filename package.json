{
  "name": "learningdoes",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "main/main.js",
  "author": "vertskap",
  "description": "Electron + NextJS example project",
  "scripts": {
    "dev": "next dev --turbopack",
